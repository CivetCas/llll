{
    "extends": "airbnb-base",
    "rules": {
        "camelcase": "off",
        "linebreak-style": "off"
    },
    "globals": {
        "axios": "readonly",
        "window":"readonly",
        "browser":"readonly",
        "chrome":"readonly"
    },
    "env": {
        "browser": true,
        "node": false
    },
    "ignorePatterns": [
        "**/vendor/*.js"
    ]
}