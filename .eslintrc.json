{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "camelcase": "off",
        "linebreak-style": "off"
    },
    "globals": {
        "window": "readonly",
        "browser": "readonly",
        "chrome": "readonly"
    },
    "ignorePatterns": [
        "**/vendor/*.js"
    ]
}